#include<iostream>
using namespace std;
int main()
{
    int n,m;
    cout<<"enter rows and coloumns";
    cin>>n>>m;
    int mat[n][m];
    cout<<"enter the array";
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cin>>mat[i][j];
        }
    }
    cout<<"The given matrix is"<<endl;;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cout<<mat[i][j]<<" ";
        }
        cout<<endl;
    }
    int temp;
    for(int i=0;i<n;i++)
    {
        for(int j=1;j<m;j++)
        {
            temp=mat[i][j];
            mat[i][j]=mat[j][i];
            mat[j][i]=temp;
        }
    }
    cout<<"The transpose of the given matrix is"<<endl;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cout<<mat[i][j]<<" ";
        }
        cout<<endl;
    }
}
